{
	"root": true,
	"env": {
		"browser": true,
		"es2021": true
	},
	"parser": "@typescript-eslint/parser",
	"plugins": ["compat", "prettier", "@typescript-eslint"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"extends": ["prettier", "eslint:recommended", "plugin:compat/recommended", "plugin:@typescript-eslint/eslint-recommended", "plugin:@typescript-eslint/recommended"],
	"rules": {
		"no-console": 1,
		"prettier/prettier": 2
	}
}
